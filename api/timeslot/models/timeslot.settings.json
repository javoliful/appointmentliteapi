{
  "kind": "collectionType",
  "collectionName": "timeslots",
  "info": {
    "name": "timeslot"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "startDateTime": {
      "type": "datetime"
    },
    "endDateTime": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "proposed",
        "merged",
        "confirmed",
        "canceled"
      ]
    },
    "appointment": {
      "via": "timeslots",
      "model": "appointment"
    },
    "bookings": {
      "via": "timeslot",
      "collection": "booking"
    }
  }
}
